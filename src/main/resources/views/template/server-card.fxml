<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="100.0" prefWidth="278.0" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <top>
       <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
           <ImageView fx:id="serverIcon" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
           <VBox prefHeight="100.0" prefWidth="180.0">
               <Label fx:id="serverName" prefHeight="48.0" prefWidth="178.0" text="Server Name" wrapText="true">
                   <font>
                       <Font name="System Bold" size="14.0" />
                   </font>
               </Label>
               <Label fx:id="serverID" prefHeight="17.0" prefWidth="179.0" text="ID" textFill="#8a8a8a" />
               <Label fx:id="serverOwner" prefHeight="17.0" prefWidth="193.0" text="Owner" />
           </VBox>
       </HBox>
   </top>
</fx:root>
